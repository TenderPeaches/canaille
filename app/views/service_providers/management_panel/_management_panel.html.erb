<%= render partial: "service_providers/management_panel/accesses", locals: { service_provider: service_provider } %>
<%= turbo_frame_tag 'service-provider-form', class: "card" do %>
    <%= tag.h3 t('keywords.edit_blank', edited: t('models.service_provider.model_name')) %>
    <%= form_with model: service_provider do |form| %>
        <%= render partial: 'service_providers/fields', locals: { service_provider: service_provider, form: form } %>
        <%= form.submit t('keywords.update') %>
    <% end %>
<% end %>
<%= render partial: "service_providers/management_panel/quotes_history", locals: { service_provider: service_provider } %>