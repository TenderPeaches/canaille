<%#(!) unused %>
<%= tag.td do %>
    <%= service_picker(form) %>
<% end %>
<%= tag.td do %>
    <%= form.number_field :min_price, min: 0, step: 1 %> $
<% end %>
<%= tag.td do %>
    <%= tag.div class: "actions" do %>
        <% if is_admin %>
            <%= form.submit t('keywords.update') %>
            <%= link_to t('keywords.delete'), service_provider_service_offer_path(service_provider, service_offer), class: "button", method: :delete %>
        <% end %>
    <% end %>
<% end %>